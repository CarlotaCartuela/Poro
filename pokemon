package org.example;

public class abstract pokemon implements pokebattle {

    private int id;

    private String nome;

    public Pokemon(int id, String nome) {
        this.id = id;
        this.nome = nome;
    }

    public int getId() {
        return id;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        System.out.println("Você não pode alterar a propriedade nome");
    }
}
